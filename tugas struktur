int l;
cout<<"_#_#_#_#_#_#_#_#_#_#_#_#_#_#_#"<<endl;
cout<<"_# Nama :Aris Kusnandar    #_#"<<endl;
cout<<"_# Nim	:5150411124        #_#"<<endl;
cout<<"_#_#_#_#_#_#_#_#_#_#_#_#_#_#_#"<<endl;


cout<<"Masukan Panjang Matriks :";
