for (int b=1;b<=l;b++){
cout<<"["<<i<<"]["<<b<<"]:";
cin>> a[i][b];
}
}

cout<<"tampilkan"<<endl;
for (int i=1;i<=l;i++){
for (int b=1;b<=l;b++){
if(i==b)
cout<< a[i][b];
else
cout<<"0";
}
cout<<endl;
}
